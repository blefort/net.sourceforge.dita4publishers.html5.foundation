<?xml version="1.0" encoding="utf-8"?><project name="dita2foundation" default="dita2foundation">

  <property name="transtype" value="foundation"></property>
  <dirname property="dita2foundation.dir" file="${ant.file.dita2foundation}"></dirname>

  <target name="dita2foundation">

    <antcall target="dita2d4p-html5">
      <param name="map2html5.style" value="${dita2foundation.dir}/xsl/map2foundation.xsl"></param>
      <param name="html5.site.theme" value="html5-foundation"></param>
      <param name="html5.theme.config.dir" value="${dita2foundation.dir}/theme"></param>
      <param name="html5.theme.dir" value="${dita2foundation.dir}/themes/html5-foundation"></param>
      <param name="html5.grid.prefix" value="large-"></param>
    </antcall>

  </target>

</project>